extends ../layout/layoutIndex

block main
  .top-space
  .row.center-xs
    .col-xs-10
      table
        thead
          tr: th(colspan="3") Usuarios registrados actualmente
        tbody
          tr
            td Nombre
            td Proveedor de datos
            td idProveedor
            td Rango
            td Correo
            td Fecha de registro
          for user in users
            tr.table
              td: p= user.username
              td: p= user.provider
              td: p= user.providerId
              td: p= user.administrator
              td: p= user.email
              td: p= user.fechaCreacion

